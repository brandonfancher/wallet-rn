{
  "presets": ["react-native"],
  "sourceMaps": true,
  "plugins": [
    ["module-resolver", {
      "alias": {
        "crypto": "react-native-crypto",
				"stream": "stream-browserify",
				"vm": "vm-browserify",
      },
    }],
  ],
  "env": {
    "development": {
      "plugins": ["transform-react-jsx-source"]
    }
  }
}
